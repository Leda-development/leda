{
  "name": "leda",
  "version": "0.0.1-alpha.5",
  "description": "UI components library for React applications",
  "main": "dist/index.js",
  "sideEffects": false,
  "types": "dist",
  "scripts": {
    "start": "npm run build:demo",
    "start:external": "npm run build:external",
    "test": "npm run jest",
    "build": "rimraf dist/ && npm run tsc && npm run sass:build",
    "build:demo": "webpack-dev-server",
    "build:external": "npm run addStyles && webpack-dev-server --host 0.0.0.0",
    "build:analyze": "webpack --mode=production --config webpack.analyze.js",
    "build:light": "webpack --config webpack.lightVersion.config.js",
    "addStyles": "rimraf ./public/assets/ && mkdirp ./public/assets && rimraf ./public/demo/esphere && mkdirp ./public/demo/esphere && ncp ./node_modules/styles-esphere/ ./public/demo/esphere && node-sass ./styles/ -o ./public/assets/css/",
    "tsc": "tsc",
    "lint": "eslint leda --color --ext .ts,.tsx",
    "prepublishOnly": "npm run build",
    "check": "npm run lint && npm run test && npm run tsc",
    "postversion": "git push origin --tags",
    "hooks:install": "ncp ./hooks ./.git/hooks",
    "hooks:uninstall": "rimraf ./.git/hooks/pre-commit ./.git/hooks/pre-push",
    "sass": "node-sass ./styles/ -o ./public/assets/css/",
    "sass:watch": "node-sass --watch ./styles/ -o ./public/assets/css/",
    "sass:build": "node-sass ./styles/ -o ./dist/styles/",
    "jest": "jest --color",
    "cy:open": "cypress open",
    "cy:test": "cypress run",
    "cypress-local": "start-test :9000 cy:open",
    "cypress-ci": "start-test :9000 cy:test"
  },
  "license": "MIT",
  "dependencies": {
    "attr-accept": "2.0.0",
    "classnames": "2.2.6",
    "element-closest": "3.0.2",
    "element-scroll-polyfill": "1.0.1",
    "create-react-class": "15.6.3",
    "history": "4.10.1",
    "lodash": "4.17.15",
    "react-css-collapse": "3.6.1",
    "react-dropzone": "10.1.8",
    "react-slider": "1.0.1",
    "svgxuse": "1.2.6"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "4.1.1",
    "@types/classnames": "2.2.9",
    "@types/enzyme": "3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "24.0.25",
    "@types/lodash": "4.14.149",
    "@types/react": "16.9.17",
    "@types/react-css-collapse": "3.6.0",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "5.1.3",
    "@types/react-slider": "1.0.0",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "2.15.0",
    "@typescript-eslint/parser": "2.15.0",
    "circular-dependency-plugin": "5.2.0",
    "core-js": "^3.6.2",
    "cypress": "^3.8.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-airbnb-typescript": "6.3.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-cypress": "2.8.1",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jest": "23.4.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "intl": "1.2.5",
    "jest": "^24.9.0",
    "jest-date-mock": "1.0.8",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "node-sass": "4.13.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "rimraf": "3.0.0",
    "shortid": "^2.2.15",
    "start-server-and-test": "^1.10.6",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-router-dom": "^5.0.1"
  }
}
