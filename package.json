{
  "name": "leda",
  "version": "0.31.2",
  "description": "UI components library for React applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git"
  },
  "scripts": {
    "start": "npm run start --prefix docs",
    "docs": "npm run dev --prefix docs",
    "build:docs": "npm run build --prefix docs",
    "install:docs": "npm install --legacy-peer-deps --prefix docs",
    "check": "npm run lint && npm run test && npm run tsc",
    "build": "npm run clean && npm run tsc && npm run sass",
    "lint": "eslint --color --ext .ts,.tsx leda",
    "test": "jest --color --silent",
    "tsc": "tsc",
    "tsc:docs": "npm run tsc --prefix docs",
    "jest": "jest",
    "sass": "node-sass --output dist/styles styles",
    "sass:build": "node-sass --output public/assets/css styles",
    "sass:watch": "node-sass --output public/assets/css styles --watch",
    "clean": "rimraf dist/*",
    "clean:assets": "rimraf public/assets && mkdirp public/assets",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npx eslint",
      "bash -c tsc --noEmit"
    ]
  },
  "dependencies": {
    "classnames": "2.2.6",
    "feather-icons-react": "0.6.2",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <19.0.0",
    "react-dom": ">=16.8.0 <19.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.0",
    "@testing-library/user-event": "8.1.3",
    "@types/classnames": "2.2.10",
    "@types/jest": "25.1.4",
    "@types/lodash": "4.14.149",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "circular-dependency-plugin": "5.2.0",
    "core-js": "3.27.2",
    "eslint": "8.46.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.0",
    "intl": "1.2.5",
    "jest": "29.6.2",
    "jest-date-mock": "1.0.8",
    "lint-staged": "^13.2.3",
    "mkdirp": "1.0.3",
    "ncp": "2.0.0",
    "node-sass": "8.0.0",
    "rimraf": "3.0.2",
    "scheduler": "^0.23.0",
    "shortid": "2.2.15",
    "start-server-and-test": "^1.15.4",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.2",
    "typescript": "^5.3.3",
    "zxcvbn": "^4.4.2"
  }
}
